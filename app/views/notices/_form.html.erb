<%= form_for notice do |f| %>
  <%= f.label :description, 'Simple Description' %>
  <%= f.text_field :description %>

  <%= f.label :details %>
  <%= f.text_area :details , rows: "3"%>
  
  <%= f.hidden_field :category %>

  <%= f.label :checkpoint_id %>
  <%= f.collection_select(:checkpoint_id, Checkpoint.all, :id, :name)%>

  <%= f.submit %>
<% end %>